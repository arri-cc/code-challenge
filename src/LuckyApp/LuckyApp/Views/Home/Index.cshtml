@model LuckyApp.Models.MyOptions

<html>
    <head>
            <link href="//fonts.googleapis.com/css?family=Roboto" rel="stylesheet">

            <style type="text/css">
                body {
                    font-family: 'Roboto', sans-serif;
                    background-color: #272822;
                    color: #66D9EF;
                }

                .centered {
                    position: fixed;
                    top: 50%;
                    left: 50%;
                    transform: translate(-50%, -50%);
                }

            </style>

        </head>
    <body>
        <h1 class="centered">@Model.MachineName @Model.HealthCheckMsg</h1>
    </body>
</html>